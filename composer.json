{
    "name": "combustible/spatial",
    "description": "Spatial data structures for PHP",
    "type": "library",
    "keywords": ["spatial", "data strucutre", "tree", "k-d tree", "clustering"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wesley Elfring",
            "email": "wesley@combustible.nl"
        }
    ],
    "autoload": {
        "psr-0" : {
            "Combustible\\Spatial" : "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/CombustibleNL/Spatial.git"
        }
    ],
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "scripts": {
        "fix-style": [
            "vendor/bin/php-cs-fixer fix"
        ]
    }
}
